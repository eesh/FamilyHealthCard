<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hussain.familyhealthcard" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".loginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".add"
            android:label="Home" >
            <intent-filter>
                <action android:name="om.example.hussain.familyhealthcard.add" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity
            android:name=".create_family"
            android:label="New Member" >
            <intent-filter>
                <action android:name="om.example.hussain.familyhealthcard.create_family" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity
            android:name=".update_details"
            android:label="Update Family Details" >
            <intent-filter>
                <action android:name="om.example.hussain.familyhealthcard.update_details" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity
            android:name=".create_family_2"
            android:label="General Details" >
            <intent-filter>
                <action android:name="om.example.hussain.familyhealthcard.create_family_2" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity
            android:name=".search_family"
            android:label="@string/title_activity_search_family" >
        </activity>
    </application>

</manifest>
