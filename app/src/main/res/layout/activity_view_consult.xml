<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#FFFFFF"
    tools:context=".MainActivity"
    android:id="@+id/add_consultation">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Time Stamp Here:"
        android:id="@+id/textView9"
        android:layout_weight="0"
        android:textColor="#000000" />

    <Space
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_weight="0"
        android:layout_gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Clinical Finding:"
                android:id="@+id/textView19"
                android:layout_weight="0.2"
                android:textColor="#000000" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=""
                android:id="@+id/textView1"
                android:layout_weight="0.2"
                android:textColor="#000000" />
            <Space
                android:layout_width="match_parent"
                android:layout_height="7dp"
                android:layout_weight="0"
                android:layout_gravity="center_horizontal" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Lab Finding:"
        android:id="@+id/textView10"
        android:layout_weight="0.2"
        android:textColor="#000000" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/textView11"
        android:layout_weight="0.2"
        android:textColor="#000000" />

    <Space
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_weight="0.1"
        android:layout_gravity="center_horizontal" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Diagnosis:"
        android:id="@+id/textView12"
        android:layout_weight="0.3"
        android:textColor="#000000" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/textView13"
        android:layout_weight="0.2"
        android:textColor="#000000" />

    <Space
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_weight="0.1"
        android:layout_gravity="center_horizontal" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Treatment:"
        android:id="@+id/textView14"
        android:layout_weight="0.3"
        android:textColor="#000000" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/textView15"
        android:layout_weight="0.2"
        android:textColor="#000000" />


    <Space
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_weight="0.1"
        android:layout_gravity="center_horizontal" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Referral:"
        android:id="@+id/textView16"
        android:layout_weight="0.3"
        android:textColor="#000000" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text=""
        android:id="@+id/textView17"
        android:layout_weight="0.2"
        android:textColor="#000000" />

    <Space
        android:layout_width="match_parent"
        android:layout_height="7dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1" />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Go to Previous Page"
        android:id="@+id/gotomain"
        android:drawableLeft="@drawable/ic_action_can"
        android:layout_gravity="center_horizontal" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>
